{
  "name": "KeyPa.ws",
  "main": "main.js",
  "version": "0.1.0",
  "build": {
    "appId": "com.mindfulpuss.keypaws",
    "nwVersion": "0.42.6",
    "packed": true,
    "targets": [
      "nsis7z"
    ],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "mac": {
      "displayName": "KeyPaws",
      "copyright": "copyright",
      "icon": "./assets/icons.icns",
      "plistStrings": {
        "CFBundleName": "KeyPaws",
        "CFBundleIdentifier": "com.mindfulpuss.keypaws",
        "CFBundleDocumentTypes": []
      }
    },
    "nsis": {
      "installDirectory": "$PROGRAMFILES\\${_COMPANYNAME}\\${_APPNAME}",
      "diffUpdaters": true
    }
  },
  "window": {
    "show_in_taskbar": false,
    "kiosk": false
  },
  "dependencies": {
    "nw": "^0.42.6"
  },
  "scripts": {
    "start": "nw --enable-logging=stderr",
    "dist": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
    "bundle": "./bundle.sh"
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.15.0"
  }
}
